<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.0</TargetFramework>
		<AssemblyName>MyRainbow</AssemblyName>
		<OutputType>Exe</OutputType>
		<PackageId>MyRainbow</PackageId>
		<UserSecretsId>aspnet-MyRainbow-20161203120550</UserSecretsId>
		<GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
		<GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
		<GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
		<AssemblyVersion>1.0.3.0</AssemblyVersion>
		<FileVersion>1.0.3.0</FileVersion>
		<Version>1.0.3</Version>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>DEBUG;TRACE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="DBScripts\CosmosDB-bulkImport.js" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="DBScripts\CosmosDB-bulkImport.js">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<None Update="DBScripts\CosmosDB-bulkDelete.js">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	<ItemGroup>
		<None Update="DBScripts\**\*">
			<CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration" Version="3.0.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.CommandLine" Version="3.0.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="3.0.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="3.0.0" />
		<PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="3.0.0" />
		<PackageReference Include="Microsoft.Data.Sqlite" Version="3.0.0" />
		<PackageReference Include="System.Data.SqlClient" Version="4.7.0" />
		<PackageReference Include="Microsoft.Azure.DocumentDB.Core" Version="2.9.0" />
		<PackageReference Include="MongoDB.Driver" Version="2.9.2" />
		<PackageReference Include="CassandraCSharpDriver" Version="3.12.0" />
		<PackageReference Include="Oracle.ManagedDataAccess.Core" Version="2.19.50" />
		<PackageReference Include="MySqlConnector" Version="0.60.4" />
		<PackageReference Include="Npgsql" Version="4.1.1" />
		<PackageReference Include="StackExchange.Redis" Version="2.0.601" />
	</ItemGroup>

